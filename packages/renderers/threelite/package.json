{
  "name": "@gs.i/three-lite-renderer",
  "_name": "@gs.i/three-lite-renderer",
  "version": "0.0.2",
  "description": "A lite version of three.js",
  "main": "build/three.min.js",
  "module": "build/three.module.js",
  "types": "types/Three.d.ts",
  "author": "浅寻",
  "keywords": [
    "WebGL",
    "three.js",
    "lite"
  ],
  "sideEffects": false,
  "files": [
    "build",
    "types",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "tnpm run dev",
    "test": "tnpm run lint && tnpm run test-unit",
    "dist": "rollup -c utils/build/rollup.config.js",
    "build-examples": "rollup -c utils/build/rollup.examples.config.js && echo '\nFormatting...' && eslint examples/js --ext js --ignore-pattern libs --ignore-pattern ifc --fix",
    "dev": "concurrently --names \"ROLLUP,HTTP\" -c \"bgBlue.bold,bgGreen.bold\" \"rollup -c utils/build/rollup.config.js -w -m inline\" \"servez -p 8080\"",
    "lint": "eslint src --ext js",
    "lint-examples": "eslint examples/js examples/jsm --ext js --ignore-pattern libs --ignore-pattern ifc",
    "lint-fix": "tnpm run lint -- --fix && tnpm run lint-examples -- --fix",
    "lint-docs": "eslint docs --ext html",
    "test-unit": "tnpm run unit --prefix test",
    "test-unit-examples": "tnpm run unit-examples --prefix test",
    "test-e2e": "node test/e2e/puppeteer.js",
    "test-e2e-cov": "node test/e2e/check-coverage.js",
    "test-treeshake": "rollup -c test/rollup.treeshake.config.js",
    "make-screenshot": "node test/e2e/puppeteer.js --make",
    "prepublishOnly": "node utils/prepublish.js",
    "tsd": "npx tsd"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "chalk": "^4.1.1",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-html": "^6.1.2",
    "glob": "^7.1.7",
    "rollup": "^2.48.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.0",
    "servez": "^1.11.0",
    "tsd": "^0.17.0"
  },
  "jspm": {
    "files": [
      "package.json",
      "LICENSE",
      "README.md",
      "build"
    ],
    "directories": {}
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "736f68fea4f78ba5424a37c46a453bda68aa0b32"
}