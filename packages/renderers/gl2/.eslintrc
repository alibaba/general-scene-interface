    {
    "extends": "airbnb/base",
    "env": {
        "browser": true,
        "mocha": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "valid-jsdoc": 2,
        "camelcase": 1,
        "no-labels": 0,
        "no-var": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "vars-on-top": 0,
        "padded-blocks": 0,
        "strict": 0,
        "no-unused-vars": 0,
        "prefer-template": 0,
        "max-len": [2, 200],
        "prefer-arrow-callback": 0,
        "space-before-function-paren": 0,
        "no-param-reassign": 0,
        "eqeqeq": 0,
        "wrap-iife": 0,
        "space-in-parens": 0,
        "object-shorthand": 0,
        "no-undef": 0,
        "no-multi-spaces": 0,
        "computed-property-spacing": 0,
        "dot-notation": 0,
        "no-use-before-define": 0,
        "prefer-const": 0,
        "no-nested-ternary": 0,
        "no-tabs": 0,
        "indent": 0,
        "no-plusplus": 0
    }
}
